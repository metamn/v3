<meta charset="utf-8">

<!-- Always force latest IE rendering engine (even in intranet) & Chrome Frame -->
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta content="width=device-width, initial-scale=1.0, maximum-scale=1" name="viewport">


<title><%= title %></title>


<%= stylesheet_link_tag 'store/all', :media => 'screen' %>
<%= stylesheet_link_tag "mobile", :media => "screen and (max-width: 640px)" %>
<%= stylesheet_link_tag "tablet", :media => "screen and (min-width: 641px) and (max-width: 1024px)" %>
<%= stylesheet_link_tag "wide", :media => "screen and (min-width: 1025px) and (max-width: 1399px)" %>
<%= stylesheet_link_tag "large", :media => "screen and (min-width: 1400px)" %>
<%= stylesheet_link_tag "portrait", :media => "all and (orientation:portrait)" %>

<%== meta_data_tags %>
<%= favicon_link_tag image_path('favicon.ico') %>
<%= csrf_meta_tags %>


<%= javascript_include_tag 'store/all' %>



<%= yield :head %>
